# VLLM configuration
HUGGING_FACE_HUB_TOKEN=your_huggingface_token_here

# Gemini configs
GEMINI_API_KEY=your_gemini_api_key_here

# LangSmith configuration (optional - for AI observability)
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=dyno-agent-production

# AWS CloudWatch (optional - for enterprise monitoring)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# Database configuration
DATABASE_URL=postgresql+asyncpg://dyno_user:dyno_pass@db:5432/dyno_db
DATABASE_URL_CHECKPOINTER=postgresql://dyno_user:dyno_pass@db:5432/dyno_db?sslmode=disable
POSTGRES_USER=dyno_user
POSTGRES_PASSWORD=dyno_pass
POSTGRES_DB=dyno_db

# Auth
JWT_SECRET=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXP_DELTA_SECONDS=3600